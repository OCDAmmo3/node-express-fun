<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<body>
  <section id="recipe-container">
    <% recipes.forEach(recipe => { %>
      <div class="recipe">
        <h2><%= recipe.name %></h2>
        <ul>
          <% recipe.ingredients.forEach(ing => { %>
            <li><%= ing %></li>
          <% }) %>
        </ul>
      </div>
    <% }) %>
  </section>
</body>
</html>